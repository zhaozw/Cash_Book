<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="star.liuwen.com.cash_books">


    <uses-permission android:name="android.permission.CAMERA"/>

    <uses-feature android:name="android.hardware.camera"/>
    <uses-feature android:name="android.hardware.camera.autofocus"/>

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>

    <uses-permission android:name="android.permission.INTERNET"/>
    <!-- 获取当前WiFi接入的状态以及WLAN热点的信息 -->
    <uses-permission android:name="android.permission.CHANGE_WIFI_MULTICAST_STATE"/>
    <!-- 改变WiFi多播状态 -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <application
        android:name=".Base.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_laun"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <!--日历-->
        <activity android:name=".Activity.CalendarActivity"/>
        <!--支出和收入-->
        <activity android:name=".Activity.IncomeAndCostActivity"/>
        <!--显示money页面-->
        <activity android:name=".Activity.PaymentActivity"/>
        <!--钱的设置页面-->
        <activity android:name=".Activity.PaySettingActivity"/>
        <!--修改的通用界面-->
        <activity android:name=".Activity.UpDateAccountCommonActivity"/>
        <!--存钱和账单总结的通用页面-->
        <activity android:name=".Activity.SaveMoneyActivity"/>
        <!--存钱计划-->
        <activity android:name=".Activity.PlanSaveMoneyActivity"/>
        <!--整理消费账单-->
        <activity android:name=".Activity.FixXiaofeiCheckActivity"/>
        <!--选择发卡行-->
        <activity android:name=".Activity.ChoiceIssuingBankActivity"/>
        <!--提醒记账页面-->
        <activity android:name=".Activity.RemindActivity"/>
        <!--新建存钱计划-->
        <activity android:name=".Activity.newSaveMoneyPlanActivity"/>
        <!--主题皮肤-->
        <activity android:name=".Activity.ChangeSkinActivity"/>
        <!--通用修改存钱计划页面-->
        <activity android:name=".Activity.UpDateSaveMoneyCommonActivity"/>
        <!--用户界面-->
        <activity android:name=".Activity.UserInfoActivity"/>
        <!--关于界面-->
        <activity android:name=".Activity.AboutMeActivity"/>
        <!--设置页面-->
        <activity android:name=".Activity.SettingActivity"/>
        <!--密码锁页面-->
        <activity android:name=".Activity.CodedLockActivity"/>
        <!--货币页面-->
        <activity android:name=".Activity.HuoBiActivity"/>
        <!--货币设置页面-->
        <activity android:name=".Activity.HuoBiSettingActivity"/>
        <!--报表明细页面-->
        <activity android:name=".Activity.ReportsDetailActivity"/>
        <!--登陆界面-->
        <activity android:name=".Activity.LoginActivity"/>
    </application>


</manifest>